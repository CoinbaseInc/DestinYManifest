<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>tiedown.ac</path>
    <nopreview/>

    <!-- Yaw -->
    <animation>
        <object-name>tiedown-unit</object-name>
        <type>rotate</type>
        <property>/sim/model/c172p/tiedowns/left/heading-deg</property>
        <factor>-1</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

    <!-- Pitch -->
    <animation>
        <object-name>tiedown-unit</object-name>
        <type>rotate</type>
        <property>/sim/model/c172p/tiedowns/left/pitch-deg</property>
        <factor>-1</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Distance -->
    <animation>
        <object-name>tiedown-unit</object-name>
        <type>scale</type>
        <property>/sim/model/c172p/tiedowns/left/length</property>
        <x-min>0.0</x-min>
        <y-min>1.0</y-min>
        <z-min>1.0</z-min>
        <x-factor>0.3846153846153846</x-factor>
        <y-factor>0.0</y-factor>
        <z-factor>0.0</z-factor>
    </animation>

</PropertyList>
