<PropertyList>

    <!-- Generic components -->
    <signals include="/Aircraft/Generic/flightrecorder/components/position.xml"/>
    <signals include="/Aircraft/Generic/flightrecorder/components/controls.xml"/>
    <signals include="/Aircraft/Generic/flightrecorder/components/environment.xml"/>
    <signals include="components/surfaces.xml"/>

    <!-- Gear -->
    <signals include="/Aircraft/Generic/flightrecorder/components/gear-fixed.xml">
        <count type="int">3</count>
    </signals>

    <!-- Engine -->
    <signals include="/Aircraft/Generic/flightrecorder/components/engine-piston-prop.xml">
        <count type="int">1</count>
    </signals>
    <signals include="/Aircraft/Generic/flightrecorder/components/faults-engines.xml">
        <count type="int">1</count>
    </signals>

    <!-- Tanks -->
    <signals include="/Aircraft/Generic/flightrecorder/components/tanks.xml">
        <count type="int">3</count>
    </signals>

    <signals>
        <!-- ============================================================== -->
        <!-- Gear Amphibious                                                -->
        <!-- ============================================================== -->
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[21]/compression-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[21]/position-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[22]/compression-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[22]/position-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[23]/compression-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[23]/position-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[24]/compression-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/gear/gear[24]/position-norm</property>
        </signal>

        <!-- ============================================================== -->
        <!-- Damage mods                                                    -->
        <!-- ============================================================== -->
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/gear/unit[0]/broken</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/gear/unit[1]/broken</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/gear/unit[2]/broken</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[4]/broken</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[5]/broken</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/contact/unit[4]/z-position</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/contact/unit[5]/z-position</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/wing-damage/left-wing</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/wing-damage/right-wing</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/crash</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/left-pontoon/damaged</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/left-pontoon/broken</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/right-pontoon/damaged</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/right-pontoon/broken</property>
        </signal>

        <!-- ============================================================== -->
        <!-- Smoke and sparks                                               -->
        <!-- ============================================================== -->

        <signal>
            <type>float</type>
            <property type="string">/velocities/groundspeed-kt</property>
        </signal>

        <!-- Wings and tail -->
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[3]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[4]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[5]/WOW</property>
        </signal>

        <!-- Gear -->
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[6]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[7]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[8]/WOW</property>
        </signal>

        <!-- Pontoon smoke -->
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[17]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[18]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[19]/WOW</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/fdm/jsbsim/contact/unit[20]/WOW</property>
        </signal>

        <!-- Pontoon wakes -->
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/contact/unit[13]/compression-ft</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/contact/unit[14]/compression-ft</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/contact/unit[15]/compression-ft</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/contact/unit[16]/compression-ft</property>
        </signal>

        <!-- ============================================================== -->
        <!-- Systems                                                        -->
        <!-- ============================================================== -->

        <signal>
            <type>float</type>
            <property type="string">/systems/vacuum/suction-inhg</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/systems/static/pressure-inhg</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/systems/pitot/measured-total-pressure-inhg</property>
        </signal>

        <!-- ============================================================== -->
        <!-- Instruments                                                    -->
        <!-- ============================================================== -->

        <!-- Attitude indicator -->
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/attitude-indicator/spin</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/attitude-indicator/horizon-offset-deg</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/instrumentation/attitude-indicator/caged-flag</property>
        </signal>

        <!-- Heading indicator -->
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/heading-indicator/spin</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/heading-indicator/offset-deg</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/autopilot/settings/heading-bug-deg</property>
        </signal>

        <!-- Magnetic compass -->
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/magnetic-compass/indicated-heading-deg</property>
        </signal>

        <!-- Altimeter -->
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/altimeter/setting-inhg</property>
        </signal>

        <!-- NAV1 and NAV2 -->
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/nav[0]/radials/selected-deg</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/nav[1]/radials/selected-deg</property>
        </signal>

        <!-- ADF -->
        <signal>
            <type>float</type>
            <property type="string">/instrumentation/adf[0]/rotation-deg</property>
        </signal>

        <!-- ============================================================== -->
        <!-- Switches                                                       -->
        <!-- ============================================================== -->

        <signal>
            <type>bool</type>
            <property type="string">/controls/engines/engine/primer-lever</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/switches/starter</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/anti-ice/pitot-heat</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/anti-ice/engine[0]/carb-heat</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/environment/aircraft-effects/cabin-heat-set</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/environment/aircraft-effects/cabin-air-set</property>
        </signal>

        <!-- Fuel selector -->
        <signal>
            <type>bool</type>
            <property type="string">/consumables/fuel/tank[0]/selected</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/consumables/fuel/tank[1]/selected</property>
        </signal>

        <!-- Passengers and baggage -->
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/inertia/pointmass-weight-lbs[0]</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/inertia/pointmass-weight-lbs[1]</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/inertia/pointmass-weight-lbs[2]</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/inertia/pointmass-weight-lbs[3]</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/fdm/jsbsim/inertia/pointmass-weight-lbs[4]</property>
        </signal>

        <!-- Lights -->
        <signal>
            <type>bool</type>
            <property type="string">/controls/lighting/nav-lights</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/lighting/beacon</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/lighting/strobe</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/lighting/landing-lights</property>
        </signal>
        <signal>
            <type>bool</type>
            <property type="string">/controls/lighting/taxi-light</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/sim/model/material/instruments/factor</property>
        </signal>

        <!-- ============================================================== -->
        <!-- Doors                                                          -->
        <!-- ============================================================== -->

        <signal>
            <type>float</type>
            <property type="string">/sim/model/door-positions/rightDoor/position-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/sim/model/door-positions/leftDoor/position-norm</property>
        </signal>
        <signal>
            <type>float</type>
            <property type="string">/sim/model/door-positions/baggageDoor/position-norm</property>
        </signal>
    </signals>

</PropertyList>
