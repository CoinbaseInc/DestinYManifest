<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <signal>
        <type>float</type>
        <property type="string">/velocities/groundspeed-kt</property>
    </signal>

    <!-- Wings and tail -->
    <signal>
        <type>bool</type>
        <property type="string">/fdm/jsbsim/contact/unit[3]/WOW</property>
    </signal>
    <signal>
        <type>bool</type>
        <property type="string">/fdm/jsbsim/contact/unit[4]/WOW</property>
    </signal>
    <signal>
        <type>bool</type>
        <property type="string">/fdm/jsbsim/contact/unit[5]/WOW</property>
    </signal>

    <!-- Gear -->
    <signal>
        <type>bool</type>
        <property type="string">/fdm/jsbsim/contact/unit[6]/WOW</property>
    </signal>
    <signal>
        <type>bool</type>
        <property type="string">/fdm/jsbsim/contact/unit[7]/WOW</property>
    </signal>
    <signal>
        <type>bool</type>
        <property type="string">/fdm/jsbsim/contact/unit[8]/WOW</property>
    </signal>

	 <!-- Gear smoke ans spark-->
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/ground/nose-gear-ground-effect-speed-kt-actual</property>
    </signal>
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/ground/left-gear-ground-effect-speed-kt-actual</property>
    </signal>
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/ground/right-gear-ground-effect-speed-kt-actual</property>
    </signal>

	 <!-- Wing smoke ans spark-->
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/ground/left-wing-ground-effect-speed-kt-actual</property>
    </signal>
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/ground/right-wing-ground-effect-speed-kt-actual</property>
    </signal>

	<!-- Tail smoke ans spark-->
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/ground/tail-ground-effect-speed-kt-actual</property>
    </signal>

    <!-- Pontoon smoke ans spark-->
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/hydro/left-ground-effect-speed-kt-actual</property>
    </signal>
	<signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/hydro/right-ground-effect-speed-kt-actual</property>
    </signal>

    <!-- Pontoon wakes -->
    <signal>
        <type>float</type>
        <property type="string">/fdm/jsbsim/hydro/spray-wake-speed-kt-actual</property>
    </signal>

</PropertyList>
